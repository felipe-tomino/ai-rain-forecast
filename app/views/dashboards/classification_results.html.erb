                      tr[i].style.display = "none";
                    } else if (split_value != "" && split_td.innerHTML != split_value) {
                      tr[i].style.display = "none";
                    } else if (mse_min_value != "" && parseFloat(mse_min_value) > parseFloat(mse_td.innerHTML)) {
