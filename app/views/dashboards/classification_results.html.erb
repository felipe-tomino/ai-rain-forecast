                var gauge_id_value = $(`#${tab_id} .${table_prefix}-gauge-id-filter-input`).val();
                var gauge_name_value = $(`#${tab_id} .${table_prefix}-gauge-name-filter-input`).val();
                var split_value = $(`#${tab_id} .${table_prefix}-split-filter-input`).val();
                var mse_min_value = $(`#${tab_id} .${table_prefix}-mse-min-filter-input`).val();
                var mse_max_value = $(`#${tab_id} .${table_prefix}-mse-max-filter-input`).val();
                var accuracy_min_value = $(`#${tab_id} .${table_prefix}-accuracy-score-min-filter-input`).val();
