                    } else if (precision_max_value != "" && parseFloat(precision_max_value) < parseFloat(precision_td.innerHTML)) {
                      tr[i].style.display = "none";
                    } else {
                      filter_rows_count++;
                      tr[i].style.display = "";
                    }
                  }
                }
                $(`#${tab_id} .filter-rows-count`).html(`${filter_rows_count}/${tr.length}`);
