                    } else if (f1_max_value != "" && parseFloat(f1_max_value) < parseFloat(f1_td.innerHTML)) {
                      tr[i].style.display = "none";
                    } else if (recall_min_value != "" && parseFloat(recall_min_value) > parseFloat(recall_td.innerHTML)) {
